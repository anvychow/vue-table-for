{
  "name": "vue-table-for",
  "description": "vue-table-for is a table builder for an array of objects, easily allowing overriding of how any aspect of the table is generated",
  "version": "0.0.5",
  "author": {
    "name": "Andrew Hunter",
    "email": "hunterae@gmail.com"
  },
  "keywords": [
    "vue",
    "vuejs",
    "vue-component",
    "table",
    "table builder",
    "table generator",
    "table-generator",
    "table-builder",
    "table builder",
    "table for",
    "slots",
    "slot hooks",
    "vue-slot-hooks"
  ],
  "homepage": "https://github.com/hunterae/vue-table-for",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hunterae/vue-table-for.git"
  },
  "bugs": {
    "url": "https://github.com/hunterae/vue-table-for/issues"
  },
  "license": "MIT",
  "main": "dist/vue-table-for.common.js",
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build-bundle": "vue-cli-service build --target lib --name vue-table-for ./src/plugin.js",
    "lint": "vue-cli-service lint --fix",
    "docs": "vue-cli-service build --dest docs",
    "gh": "gh-pages -d docs",
    "publish-docs": "./bin/publish_docs.sh"
  },
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.4",
    "@vue/cli-plugin-eslint": "^3.0.4",
    "@vue/cli-service": "^3.0.4",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "deepmerge": "^2.2.1",
    "eslint": "^5.8.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "faker": "^4.1.0",
    "gh-pages": "^2.0.1",
    "github-markdown-css": "^2.10.0",
    "node-sass": "^4.10.0",
    "prettier": "^1.15.3",
    "prismjs": "^1.15.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^7.1.0",
    "vue": "^2.5.17",
    "vue-markdown-loader": "^2.4.1",
    "vue-prism-component": "^1.1.1",
    "vue-template-compiler": "^2.5.17"
  },
  "peerDependencies": {
    "deepmerge": ">=2.0.0",
    "vue": ">=2.1.0"
  },
  "engines": {
    "node": ">=4",
    "npm": ">=3"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
